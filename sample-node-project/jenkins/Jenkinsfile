pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  stages {
    stage('Test') {
      steps {
        sh 'node --version'
      }
    }
	stage('Npm Install') {
      steps {
        sh 'npm install'
      }
    }
	stage('Npm Start') {
      steps {
        sh 'npm start'
      }
    }
	
  }
}
